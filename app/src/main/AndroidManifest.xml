<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gu.task">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".androidmainifest.MainifestMainActivity" />
        <activity android:name=".intentflag.IntentFlagMainActivity" />
        <activity android:name=".androidmainifest.standard.StandardModeActivity" />
        <activity android:name=".androidmainifest.singletask.NormalActivity" />
        <activity
            android:name=".androidmainifest.singletop.SingleTopActivity"
            android:launchMode="singleTop" />
        <activity
            android:name=".androidmainifest.singletask.SingleTaskActivity"
            android:launchMode="singleTask" />
        <activity
            android:name=".androidmainifest.singleinstance.SingleInstaceActivity"
            android:launchMode="singleInstance" />
        <activity
            android:name=".intentflag.NewTaskActivity" />
    </application>

</manifest>
